<%= simple_form_for(@other) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :other_name %>
    <%= f.input :price %>
    <%= f.input :description %>
    <%= f.input :quantity %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
